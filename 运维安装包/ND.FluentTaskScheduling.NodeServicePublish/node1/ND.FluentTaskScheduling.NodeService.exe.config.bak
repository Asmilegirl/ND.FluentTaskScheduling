<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!--任务调度网站站点url-->
    <add key="TaskApiUrl" value="http://localhost:21794/api/" />
    <!--当前节点id 与管理站点的id号对应-->
    <add key="NodeID" value="7" />
    <add key="WindowsServiceDiscription" value="分布式任务调度程序windows服务（负责task的启动,停止,恢复,释放,预警等功能）" />
